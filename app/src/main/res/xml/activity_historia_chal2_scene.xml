<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:autoTransition="animateToEnd"
        motion:duration="6000">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/v2_top"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/v2_top"
               motion:framePosition="25"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/v3_med"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/v3_med"
               motion:framePosition="25"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/v3_med"
               motion:framePosition="50"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/v4_izq"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/v4_izq"
               motion:framePosition="25"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/v4_izq"
               motion:framePosition="50"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/v4_izq"
               motion:framePosition="75"
               android:alpha="1" />
           <KeyAttribute
               motion:motionTarget="@+id/v5_der"
               motion:framePosition="0"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/v5_der"
               motion:framePosition="25"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/v5_der"
               motion:framePosition="50"
               android:alpha="0" />
           <KeyAttribute
               motion:motionTarget="@+id/v5_der"
               motion:framePosition="75"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/v5_der"
               motion:framePosition="96"
               android:alpha="1" />
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>